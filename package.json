{
  "name": "cypress-ncatestify-plugin",
  "version": "2.0.7",
  "description": "NCA TESTIFY commands and tests",
  "main": "src",
  "types": "src",
  "files": [
    "**/*.js",
    "index.d.ts"
  ],
  "scripts": {
    "typecheck": "tsc --skipLibCheck --noEmit",
    "build": "tsc --skipLibCheck",
    "lint": "eslint --ext=.js,.ts,.tsx",
    "release": "npm build && npm version && node scripts/release.js && npm publish",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "keywords": [],
  "author": "Roland Golla",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.2.0",
    "eslint": "^8.30.0",
    "typescript": "^4.8.4"
  }
}
